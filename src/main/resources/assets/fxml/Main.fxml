<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1000.0" spacing="15.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.pixellauncher.controller.MainController">
   <ScrollPane fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets left="25.0" right="25.0" top="25.0" />
      </VBox.margin>
      <FlowPane prefHeight="200.0" prefWidth="200.0" />
   </ScrollPane>
   <AnchorPane prefHeight="30.0">
      <VBox.margin>
         <Insets bottom="15.0" left="15.0" right="15.0" />
      </VBox.margin>
      <Button mnemonicParsing="false" onAction="#handleAccountButton" text="Account" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Button mnemonicParsing="false" onAction="#handleSettingsButton" text="Settings" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
   </AnchorPane>
</VBox>
